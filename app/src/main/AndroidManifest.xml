<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cybeleminigames">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CYBELEMINIGAMES">
        <activity
            android:name=".fingerbattle.FingerBattle"
            android:exported="false" />
        <activity
            android:name=".towerofhanoi.HanoiTower"
            android:exported="false" />
        <activity
            android:name=".userprofile.UserProfile"
            android:exported="false" />
        <activity
            android:name=".login.Register"
            android:exported="false" />
        <activity
            android:name=".login.Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".tictactoe.ZeroKaata"
            android:exported="false" />
        <activity
            android:name=".towerofhanoi.TowerOfHanoi"
            android:exported="false" />
        <activity
            android:name=".stonepaperscissor.RockPaper"
            android:exported="false" />
        <activity
            android:name=".tictactoe.TicTacToe"
            android:exported="false" />
        <activity
            android:name=".selectscreen.SelectGame"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>